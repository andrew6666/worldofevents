<div class="event-element col-md-4">
    <div class="event-title">
        <a href="{{ path('woe_web_event', {'id': event.id}) }}">{{ event.title }}</a>
    </div>
    <div class="event-info">
        <a href="{{ path('woe_web_event', {'id': event.id}) }}">
            <img src="{{ event.image }}" class="event-image">
        </a>
    </div>
    <div class="event-icons">
        <div class="event-icon-row">
            <span class="glyphicon glyphicon-calendar"></span>
            <span class="event-info">Šiandien, 19:00</span></div>
        <div class="event-icon-row">
            <span class="glyphicon glyphicon-map-marker"></span>
            <span class="event-info">Lietuvos nacionalinė filharmonija, Vilnius</span>
        </div>
        <div class="event-icon-row">
            <span class="glyphicon glyphicon-euro"></span>
            <span class="event-info">6.81 - 11.15&euro;</span></div>
    </div>
</div>
